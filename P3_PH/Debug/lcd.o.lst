 357:../lcd.c      **** void Lcd_DspAscII8x16(INT16U x0, INT16U y0, INT8U ForeColor, INT8U * s)
 358:../lcd.c      **** {
 359:../lcd.c      **** 	INT16 i,j,k,x,y,xx;
 360:../lcd.c      **** 	INT8U qm;
 361:../lcd.c      **** 	INT32U ulOffset;
 362:../lcd.c      **** 	INT8 ywbuf[16],temp[2];
 363:../lcd.c      ****     
 364:../lcd.c      **** 	for( i = 0; i < strlen((const char*)s); i++ )
 365:../lcd.c      **** 	{
 366:../lcd.c      **** 		if( (INT8U)*(s+i) >= 161 )
 367:../lcd.c      **** 		{
 368:../lcd.c      **** 			temp[0] = *(s + i);
 369:../lcd.c      **** 			temp[1] = '\0';
 370:../lcd.c      **** 			return;
 371:../lcd.c      **** 		}
 372:../lcd.c      **** 		else
 373:../lcd.c      **** 		{
 374:../lcd.c      **** 			qm = *(s+i);
 375:../lcd.c      **** 			ulOffset = (INT32U)(qm) * 16;		//Here to be changed tomorrow
