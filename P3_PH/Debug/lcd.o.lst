 199:../lcd.c      **** void Lcd_Draw_Line(INT16 usX0, INT16 usY0, INT16 usX1, INT16 usY1, INT8U ucColor, INT16U usWidth)
 200:../lcd.c      **** {
 201:../lcd.c      **** 	INT16 usDx;
 202:../lcd.c      **** 	INT16 usDy;
 203:../lcd.c      **** 	INT16 y_sign;
 204:../lcd.c      **** 	INT16 x_sign;
 205:../lcd.c      **** 	INT16 decision;
 206:../lcd.c      **** 	INT16 wCurx, wCury, wNextx, wNexty, wpy, wpx;
 207:../lcd.c      **** 
 208:../lcd.c      **** 	if( usY0 == usY1 )
 209:../lcd.c      **** 	{
 210:../lcd.c      **** 		Lcd_Draw_HLine (usX0, usX1, usY0, ucColor, usWidth);
 211:../lcd.c      **** 		return;
 212:../lcd.c      **** 	}
 213:../lcd.c      **** 	if( usX0 == usX1 )
 214:../lcd.c      **** 	{
 215:../lcd.c      **** 		Lcd_Draw_VLine (usY0, usY1, usX0, ucColor, usWidth);
 216:../lcd.c      **** 		return;
 217:../lcd.c      **** 	}
 218:../lcd.c      **** 	usDx = abs(usX0 - usX1);
 219:../lcd.c      **** 	usDy = abs(usY0 - usY1);
 220:../lcd.c      **** 	if( ((usDx >= usDy && (usX0 > usX1)) ||
 221:../lcd.c      ****         ((usDy > usDx) && (usY0 > usY1))) )
 222:../lcd.c      ****     {
 223:../lcd.c      ****         GUISWAP(usX1, usX0);
 224:../lcd.c      ****         GUISWAP(usY1, usY0);
 225:../lcd.c      ****     }
 226:../lcd.c      ****     y_sign = (usY1 - usY0) / usDy;
 227:../lcd.c      ****     x_sign = (usX1 - usX0) / usDx;
 228:../lcd.c      **** 
 229:../lcd.c      ****     if( usDx >= usDy )
 230:../lcd.c      ****     {
 231:../lcd.c      ****         for( wCurx = usX0, wCury = usY0, wNextx = usX1,
 232:../lcd.c      ****              wNexty = usY1, decision = (usDx >> 1);
 233:../lcd.c      ****              wCurx <= wNextx; wCurx++, wNextx--, decision += usDy )
 234:../lcd.c      ****         {
 235:../lcd.c      ****             if( decision >= usDx )
 236:../lcd.c      ****             {
 237:../lcd.c      ****                 decision -= usDx;
 238:../lcd.c      ****                 wCury += y_sign;
 239:../lcd.c      ****                 wNexty -= y_sign;
 240:../lcd.c      ****             }
 241:../lcd.c      ****             for( wpy = wCury - usWidth / 2;
 242:../lcd.c      ****                  wpy <= wCury + usWidth / 2; wpy++ )
 243:../lcd.c      ****             {
 244:../lcd.c      ****                 (LCD_PutPixel(wCurx, wpy, ucColor));
 245:../lcd.c      ****             }
 246:../lcd.c      **** 
 247:../lcd.c      ****             for( wpy = wNexty - usWidth / 2;
 248:../lcd.c      ****                  wpy <= wNexty + usWidth / 2; wpy++ )
 249:../lcd.c      ****             {
 250:../lcd.c      ****                 (LCD_PutPixel(wNextx, wpy, ucColor));
 251:../lcd.c      ****             }
 252:../lcd.c      ****         }
 253:../lcd.c      ****     }
 254:../lcd.c      ****     else
 255:../lcd.c      ****     {
 256:../lcd.c      ****         for( wCurx = usX0, wCury = usY0, wNextx = usX1,
 257:../lcd.c      ****              wNexty = usY1, decision = (usDy >> 1);
 258:../lcd.c      ****              wCury <= wNexty; wCury++, wNexty--, decision += usDx )
 259:../lcd.c      ****         {
 260:../lcd.c      ****             if( decision >= usDy )
 261:../lcd.c      ****             {
 262:../lcd.c      ****                 decision -= usDy;
 263:../lcd.c      ****                 wCurx += x_sign;
 264:../lcd.c      ****                 wNextx -= x_sign;
 265:../lcd.c      ****             }
 266:../lcd.c      ****             for( wpx = wCurx - usWidth / 2;
 267:../lcd.c      ****                  wpx <= wCurx + usWidth / 2; wpx++ )
 268:../lcd.c      ****             {
 269:../lcd.c      ****                 (LCD_PutPixel(wpx, wCury, ucColor));
 270:../lcd.c      ****             }
 271:../lcd.c      **** 
 272:../lcd.c      ****             for( wpx = wNextx - usWidth / 2;
 273:../lcd.c      ****                  wpx <= wNextx + usWidth / 2; wpx++ )
 274:../lcd.c      ****             {
 275:../lcd.c      ****                 (LCD_PutPixel(wpx, wNexty, ucColor));
 276:../lcd.c      ****             }
 277:../lcd.c      ****         }
 278:../lcd.c      ****     }
 279:../lcd.c      **** }
