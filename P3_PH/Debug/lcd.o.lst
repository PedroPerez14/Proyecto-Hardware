 112:../lcd.c      **** * name:		LcdClrRect()
 113:../lcd.c      **** * func:		fill appointed area with appointed color
 114:../lcd.c      **** * para:		usLeft,usTop,usRight,usBottom -- area's rectangle acme coordinate
 115:../lcd.c      **** *			ucColor -- appointed color value
 116:../lcd.c      **** * ret:		none
 117:../lcd.c      **** * modify:
 118:../lcd.c      **** * comment:	also as clear screen function 
 119:../lcd.c      **** *********************************************************************************************/
 120:../lcd.c      **** void LcdClrRect(INT16 usLeft, INT16 usTop, INT16 usRight, INT16 usBottom, INT8U ucColor)
 121:../lcd.c      **** {
 122:../lcd.c      **** 	volatile INT16 i,k,l,m;
 123:../lcd.c      **** 	
 124:../lcd.c      **** 	INT32U ulColor = (ucColor << 28) | (ucColor << 24) | (ucColor << 20) | (ucColor << 16) | 
 125:../lcd.c      **** 				     (ucColor << 12) | (ucColor << 8) | (ucColor << 4) | ucColor;
 126:../lcd.c      **** 
 127:../lcd.c      **** 	i = k = l = m = 0;	
 128:../lcd.c      **** 	if( (usRight-usLeft) <= 8 )
 129:../lcd.c      **** 	{
 130:../lcd.c      **** 		for( i=usTop; i<=usBottom; i++)
 131:../lcd.c      **** 		{
 132:../lcd.c      **** 			for( m=usLeft; m<=usRight; m++)
 133:../lcd.c      **** 			{
 134:../lcd.c      **** 				(LCD_PutPixel(m, i, ucColor));
 135:../lcd.c      **** 			}
 136:../lcd.c      **** 		}	
 137:../lcd.c      **** 		return;
 138:../lcd.c      **** 	}
 139:../lcd.c      **** 
 140:../lcd.c      **** 	/* check borderline */
 141:../lcd.c      **** 	if( 0 == (usLeft%8) )
 142:../lcd.c      **** 		k=usLeft;
 143:../lcd.c      **** 	else
 144:../lcd.c      **** 	{
 145:../lcd.c      **** 		k=(usLeft/8)*8+8;
 146:../lcd.c      **** 	}
 147:../lcd.c      **** 	if( 0 == (usRight%8) )
 148:../lcd.c      **** 		l= usRight;
 149:../lcd.c      **** 	else
 150:../lcd.c      **** 	{
 151:../lcd.c      **** 		l=(usRight/8)*8;
 152:../lcd.c      **** 	}
 153:../lcd.c      **** 
 154:../lcd.c      **** 	for( i=usTop; i<=usBottom; i++ )
 155:../lcd.c      **** 	{
 156:../lcd.c      **** 		for( m=usLeft; m<=(k-1); m++ )
 157:../lcd.c      **** 		{
 158:../lcd.c      **** 			(LCD_PutPixel(m, i, ucColor));
 159:../lcd.c      **** 		}
 160:../lcd.c      **** 		for( m=k; m<l; m+=8 )
 161:../lcd.c      **** 		{
 162:../lcd.c      **** 			(*(INT32U*)(LCD_VIRTUAL_BUFFER + i * SCR_XSIZE / 2 + m / 2)) = ulColor;
 163:../lcd.c      **** 		}
 164:../lcd.c      **** 		for( m=l; m<=usRight; m++ )
 165:../lcd.c      **** 		{
 166:../lcd.c      **** 			(LCD_PutPixel(m, i, ucColor));
 167:../lcd.c      **** 		}
 168:../lcd.c      **** 	}
 169:../lcd.c      **** }
