 260:../reversi8_2019.c **** // Devuelve el contenido de la posición indicadas por la fila y columna actual.
 261:../reversi8_2019.c **** // Además informa si la posición es válida y contiene alguna ficha.
 262:../reversi8_2019.c **** // Esto lo hace por referencia (en *posicion_valida)
 263:../reversi8_2019.c **** // Si devuelve un 0 no es válida o está vacia.
 264:../reversi8_2019.c **** char ficha_valida(char tablero[][DIM], char f, char c, int *posicion_valida)
 265:../reversi8_2019.c **** {
 266:../reversi8_2019.c ****     char ficha;
 267:../reversi8_2019.c ****     if ((f < DIM) && (c < DIM) && (tablero[f][c] != CASILLA_VACIA))
 268:../reversi8_2019.c ****     {
 269:../reversi8_2019.c ****         *posicion_valida = 1;
 270:../reversi8_2019.c ****         ficha = tablero[f][c];
 271:../reversi8_2019.c ****     }
 272:../reversi8_2019.c ****     else
 273:../reversi8_2019.c ****     {
 274:../reversi8_2019.c ****         *posicion_valida = 0;
 275:../reversi8_2019.c ****         ficha = CASILLA_VACIA;
 276:../reversi8_2019.c ****     }
 277:../reversi8_2019.c ****     return ficha;
 278:../reversi8_2019.c **** }
