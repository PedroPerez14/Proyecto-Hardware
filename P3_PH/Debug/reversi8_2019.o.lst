 346:../reversi8_2019.c **** // f y c son la fila y columna a analizar
 347:../reversi8_2019.c **** // char vSF[DIM] = {-1,-1, 0, 1, 1, 1, 0,-1};
 348:../reversi8_2019.c **** // char vSC[DIM] = { 0, 1, 1, 1, 0,-1,-1,-1};
 349:../reversi8_2019.c **** int actualizar_tablero(char tablero[][DIM], char f, char c, char color)
 350:../reversi8_2019.c **** {
 351:../reversi8_2019.c ****     char SF, SC; // cantidades a sumar para movernos en la dirección que toque
 352:../reversi8_2019.c ****     int i, flip, patron;
 353:../reversi8_2019.c **** 
 354:../reversi8_2019.c ****     for (i = 0; i < DIM; i++) // 0 es Norte, 1 NE, 2 E ...
 355:../reversi8_2019.c ****     {
 356:../reversi8_2019.c ****         SF = vSF[i];
 357:../reversi8_2019.c ****         SC = vSC[i];
 358:../reversi8_2019.c ****         // flip: numero de fichas a voltear
 359:../reversi8_2019.c ****         flip = 0;
 360:../reversi8_2019.c ****         patron = patron_volteo(tablero, &flip, f, c, SF, SC, color);
