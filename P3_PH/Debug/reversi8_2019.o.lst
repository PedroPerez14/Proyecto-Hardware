 337:../reversi8_2019.c ****     {
 338:../reversi8_2019.c ****         FA = FA + SF;
 339:../reversi8_2019.c ****         CA = CA + SC;
 340:../reversi8_2019.c ****         tablero[FA][CA] = color;
 341:../reversi8_2019.c ****     }
 342:../reversi8_2019.c **** }
