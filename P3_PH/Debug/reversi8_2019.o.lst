 371:../reversi8_2019.c **** // Recorre todo el tablero comprobando en cada posición si se puede mover
 372:../reversi8_2019.c **** // En caso afirmativo, consulta la puntuación de la posición y si es la mejor
 373:../reversi8_2019.c **** // que se ha encontrado la guarda
 374:../reversi8_2019.c **** // Al acabar escribe el movimiento seleccionado en f y c
 375:../reversi8_2019.c **** 
 376:../reversi8_2019.c **** // Candidatas
 377:../reversi8_2019.c **** // NO    0
 378:../reversi8_2019.c **** // SI    1
 379:../reversi8_2019.c **** // CASILLA_OCUPADA 2
 380:../reversi8_2019.c **** int elegir_mov(char candidatas[][DIM], char tablero[][DIM], char *f, char *c)
 381:../reversi8_2019.c **** {
 382:../reversi8_2019.c ****     int i, j, k, found;
 383:../reversi8_2019.c ****     int mf = -1; // almacena la fila del mejor movimiento encontrado
 384:../reversi8_2019.c ****     int mc;      // almacena la columna del mejor movimiento encontrado
 385:../reversi8_2019.c ****     char mejor = 0; // almacena el mejor valor encontrado
 386:../reversi8_2019.c ****     int patron, longitud;
 387:../reversi8_2019.c ****     char SF, SC; // cantidades a sumar para movernos en la dirección que toque
 388:../reversi8_2019.c **** 
 389:../reversi8_2019.c ****     // Recorremos todo el tablero comprobando dónde podemos mover
 390:../reversi8_2019.c ****     // Comparamos la puntuación de los movimientos encontrados y nos quedamos con el mejor
 391:../reversi8_2019.c ****     for (i=0; i<DIM; i++)
 392:../reversi8_2019.c ****     {
 393:../reversi8_2019.c ****         for (j=0; j<DIM; j++)
 394:../reversi8_2019.c ****         {   // indica en qué casillas quizá se pueda mover
 395:../reversi8_2019.c ****             if (candidatas[i][j] == SI)
 396:../reversi8_2019.c ****             {
 397:../reversi8_2019.c ****                 if (tablero[i][j] == CASILLA_VACIA)
 398:../reversi8_2019.c ****                 {
 399:../reversi8_2019.c ****                     found = 0;
 400:../reversi8_2019.c ****                     k = 0;
 401:../reversi8_2019.c **** 
 402:../reversi8_2019.c ****                     // en este bucle comprobamos si es un movimiento válido
 403:../reversi8_2019.c ****                     // (es decir si implica voltear en alguna dirección)
 404:../reversi8_2019.c ****                     while ((found == 0) && (k < DIM))
 405:../reversi8_2019.c ****                     {
 406:../reversi8_2019.c ****                         SF = vSF[k];    // k representa la dirección que miramos
 407:../reversi8_2019.c ****                         SC = vSC[k];    // 1 es norte, 2 NE, 3 E ...
 408:../reversi8_2019.c **** 
 409:../reversi8_2019.c ****                         // nos dice qué hay que voltear en cada dirección
 410:../reversi8_2019.c ****                         longitud = 0;
 411:../reversi8_2019.c ****                         patron = patron_volteo(tablero, &longitud, i, j, SF, SC, FICHA_BLANCA);
 412:../reversi8_2019.c ****                         if (patron == PATRON_ENCONTRADO)
 413:../reversi8_2019.c ****                         {
 414:../reversi8_2019.c ****                             found = 1;
 415:../reversi8_2019.c ****                             if (tabla_valor[i][j] > mejor)
 416:../reversi8_2019.c ****                             {
 417:../reversi8_2019.c ****                                 mf = i;
 418:../reversi8_2019.c ****                                 mc = j;
 419:../reversi8_2019.c ****                                 mejor = tabla_valor[i][j];
 420:../reversi8_2019.c ****                             }
 421:../reversi8_2019.c ****                         }
 422:../reversi8_2019.c ****                         k++;
 423:../reversi8_2019.c ****                         // si no hemos encontrado nada probamos con la siguiente dirección
 424:../reversi8_2019.c ****                     }
 425:../reversi8_2019.c ****                 }
 426:../reversi8_2019.c ****             }
 427:../reversi8_2019.c ****         }
 428:../reversi8_2019.c ****     }
 429:../reversi8_2019.c ****     *f = (char) mf;
 430:../reversi8_2019.c ****     *c = (char) mc;
 431:../reversi8_2019.c ****     // si no se ha encontrado una posición válida devuelve -1
 432:../reversi8_2019.c ****     return mf;
 433:../reversi8_2019.c **** }
