 362:../reversi8_2019.c ****         {
 363:../reversi8_2019.c ****             voltear(tablero, f, c, SF, SC, flip, color);
 364:../reversi8_2019.c ****             jugada_valida = 1;          //TODO explicar
 365:../reversi8_2019.c ****         }
 366:../reversi8_2019.c ****     }
