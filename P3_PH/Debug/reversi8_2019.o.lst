 281:../reversi8_2019.c **** // La función patrón volteo comprueba si hay que actualizar una determinada direccion,
 282:../reversi8_2019.c **** // busca el patrón de volteo (n fichas del rival seguidas de una ficha del jugador actual)
 283:../reversi8_2019.c **** // en una dirección determinada
 284:../reversi8_2019.c **** // SF y SC son las cantidades a sumar para movernos en la dirección que toque
 285:../reversi8_2019.c **** // color indica el color de la pieza que se acaba de colocar
 286:../reversi8_2019.c **** // la función devuelve PATRON_ENCONTRADO (1) si encuentra patrón y NO_HAY_PATRON (0) en caso contra
 287:../reversi8_2019.c **** // FA y CA son la fila y columna a analizar
 288:../reversi8_2019.c **** // longitud es un parámetro por referencia. Sirve para saber la longitud del patrón que se está ana
 289:../reversi8_2019.c **** //          Se usa para saber cuantas fichas habría que voltear
 290:../reversi8_2019.c **** int patron_volteo(char tablero[][DIM], int *longitud, char FA, char CA, char SF, char SC, char colo
 291:../reversi8_2019.c **** {
 292:../reversi8_2019.c **** 	int t_inicio = timer2_leer();
 293:../reversi8_2019.c **** 	int t_final;
 294:../reversi8_2019.c **** 	int posicion_valida; // indica si la posición es valida y contiene una ficha de algún jugador
 295:../reversi8_2019.c **** 	char casilla;   // casilla es la casilla que se lee del tablero
 296:../reversi8_2019.c **** 
 297:../reversi8_2019.c **** 	FA = FA + SF;
 298:../reversi8_2019.c **** 	CA = CA + SC;
 299:../reversi8_2019.c **** 	casilla = ficha_valida(tablero, FA, CA, &posicion_valida);
 300:../reversi8_2019.c **** 	while ((posicion_valida == 1) && (casilla != color))
 301:../reversi8_2019.c **** 	// mientras la casilla está en el tablero, no está vací­a,
 302:../reversi8_2019.c **** 	// y es del color rival seguimos buscando el patron de volteo
 303:../reversi8_2019.c **** 	{
 304:../reversi8_2019.c **** 		FA = FA + SF;
 305:../reversi8_2019.c **** 		CA = CA + SC;
 306:../reversi8_2019.c **** 		*longitud = *longitud + 1;
 307:../reversi8_2019.c **** 		casilla = ficha_valida(tablero, FA, CA, &posicion_valida);
 308:../reversi8_2019.c **** 	}
 309:../reversi8_2019.c ****     // si la ultima posición era válida y la ficha es del jugador actual,
 310:../reversi8_2019.c ****     // entonces hemos encontrado el patrón
 311:../reversi8_2019.c **** 	if ((posicion_valida == 1) && (casilla == color) && (*longitud >0))
 312:../reversi8_2019.c **** 	{
 313:../reversi8_2019.c **** 		t_final = timer2_leer();
 314:../reversi8_2019.c **** 		t_pv += (t_final - t_inicio);
 315:../reversi8_2019.c **** 		veces_pv++;
 316:../reversi8_2019.c **** 		return PATRON_ENCONTRADO; // si hay que voltear una ficha o más hemos encontrado el patrón
 317:../reversi8_2019.c **** 	}
 318:../reversi8_2019.c **** 	else
 319:../reversi8_2019.c **** 	{
 320:../reversi8_2019.c **** 		t_final = timer2_leer();
 321:../reversi8_2019.c **** 		t_pv += (t_final - t_inicio);
 322:../reversi8_2019.c **** 		veces_pv++;
 323:../reversi8_2019.c **** 		return NO_HAY_PATRON; // si no hay que voltear no hay patrón
 324:../reversi8_2019.c **** 	}
 325:../reversi8_2019.c **** }
